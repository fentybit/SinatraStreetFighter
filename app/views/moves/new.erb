<form method="POST" action="/<%= @character.slug %>/moves/new">
<% Move.all.each do |move| %>

    <input type="checkbox" name="character[move_ids][]" value="<%= move.id %>"> <%= move.name %><br>
<% end %>

<br>
New fighting move
<input type="text" class="form-control" placeholder="New fighting move" name="move[name]">
<br>

<input type="submit" class="btn btn-outline-warning" value="Add Fighting Moves">
</form>